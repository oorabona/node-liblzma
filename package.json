{
  "name": "node-liblzma",
  "version": "1.1.0",
  "description": "NodeJS wrapper for liblzma",
  "main": "./lib/lzma.js",
  "scripts": {
    "install": "node-gyp install",
    "postinstall": "run-p build:*",
    "build:native": "node-gyp rebuild",
    "build:coffee": "coffee --compile --output lib/ src/",
    "rebuild": "node-gyp rebuild",
    "prebuild": "prebuildify --napi --strip",
    "prebuild:debug": "prebuildify --napi --debug",
    "test": "npx mocha test/*.coffee"
  },
  "dependencies": {
    "node-addon-api": "4.2.0",
    "node-gyp-build": "4.3.0",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "coffeescript": "2.6.1",
    "expect.js": "0.3.1",
    "mocha": "9.2.1",
    "node-gyp": "8.4.1",
    "prebuildify": "5.0.0",
    "tsd": "0.19.1"
  },
  "types": "index.d.ts",
  "engines": {
    "node": ">12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/node-liblzma"
  },
  "keywords": [
    "module",
    "lzma",
    "lzma2",
    "xz",
    "unxz",
    "compress",
    "uncompress",
    "decompress",
    "filters",
    "binding",
    "native"
  ],
  "gypfile": true,
  "author": "Olivier ORABONA",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/oorabona/node-liblzma/issues"
  },
  "homepage": "https://github.com/oorabona/node-liblzma",
  "binary": {
    "module_name": "liblzma"
  },
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "coffeescript/register"
    ],
    "inline-diffs": true,
    "timeout": 5000,
    "reporter": "spec",
    "recursive": true
  }
}
