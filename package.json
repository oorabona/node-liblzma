{
  "name": "node-liblzma",
  "version": "1.0.6",
  "description": "NodeJS wrapper for liblzma",
  "main": "./lib/lzma.js",
  "scripts": {
    "test": "./node_modules/.bin/ubs test",
    "install": "./node_modules/.bin/ubs install",
    "preinstall": "npm install node-pre-gyp",
    "prepublish": "./node_modules/.bin/ubs build_coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oorabona/node-liblzma"
  },
  "keywords": [
    "module",
    "lzma",
    "lzma2",
    "xz",
    "unxz",
    "compress",
    "uncompress",
    "decompress",
    "filters",
    "binding",
    "native"
  ],
  "gypfile": true,
  "author": "Olivier ORABONA",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/oorabona/node-liblzma/issues"
  },
  "homepage": "https://github.com/oorabona/node-liblzma",
  "dependencies": {
    "nan": "latest",
    "node-pre-gyp": "latest",
    "ubs": "latest"
  },
  "devDependencies": {
    "coffee-script": "*",
    "expect.js": "^0.3.1",
    "mocha": "^2.0.1",
    "q": "^1.0.1"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "build.yml",
    "lib",
    "build/Release/node-liblzma.node"
  ],
  "binary": {
    "module_name": "node-liblzma",
    "module_path": "./build/",
    "host": "https://github.com/",
    "remote_path": "./oorabona/node-liblzma/"
  }
}
